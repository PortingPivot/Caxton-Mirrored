{
  "schemaVersion": 1,
  "id": "caxton",
  "version": "${version}",
  "name": "Caxton",
  "description": "Adds TTF and OTF font support to Minecraft.",
  "authors": [
    "+merlan #flirora"
  ],
  "contact": {
    "sources": "https://gitlab.com/Kyarei/caxton",
    "homepage": "https://gitlab.com/Kyarei/caxton",
    "issues": "https://gitlab.com/Kyarei/caxton/-/issues"
  },
  "license": "MIT",
  "icon": "assets/caxton/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "xyz.flirora.caxton.CaxtonModClient"
    ],
    "main": [
      "xyz.flirora.caxton.CaxtonMod"
    ],
    "mm:early_risers": [
      "xyz.flirora.caxton.CaxtonEarlyRiser"
    ]
  },
  "mixins": [
    "caxton.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.11",
    "fabric-api": "*",
    "minecraft": "1.19.3-beta.3"
  },
  "accessWidener": "caxton.accesswidener"
}
